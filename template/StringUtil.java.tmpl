package {{context.group}}.{{context.project}}.fun;

public class StringUtil {
    public static String indentLine(int num, String str) {
        StringBuilder builder = new StringBuilder();
        for (int i = 0; i < num; ++i) {
            builder.append('\t');
        }
        builder.append(str);
        builder.append('\n');
        return builder.toString();
    }
}
